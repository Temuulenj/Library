<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Library</display-name>
  <filter>
    <filter-name>CORSFilter</filter-name>
    <filter-class>com.xxzhi.Filter.CORSFilter</filter-class>
  </filter>
  <filter-mapping>
    <filter-name>CORSFilter</filter-name>
    <url-pattern>/</url-pattern>
  </filter-mapping>
  <servlet>
    <servlet-name>getAllSeat</servlet-name>
    <servlet-class>com.xxzhi.servlet.GetAllSeat</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getSeatByFloor</servlet-name>
    <servlet-class>com.xxzhi.servlet.GetSeatByFloor</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>getAllSeatInfo</servlet-name>
    <servlet-class>com.xxzhi.servlet.GetAllSeatInfo</servlet-class>
  </servlet>
  <servlet>
    <servlet-name>reserve</servlet-name>
    <servlet-class>com.xxzhi.servlet.ReserveServlet</servlet-class>
  </servlet>
  <servlet-mapping>
    <servlet-name>reserve</servlet-name>
    <url-pattern>/api/reserve</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getAllSeatInfo</servlet-name>
    <url-pattern>/api/getSeatInfo</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getSeatByFloor</servlet-name>
    <url-pattern>/api/getByFloor</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>getAllSeat</servlet-name>
    <url-pattern>/api/getAllSeat</url-pattern>
  </servlet-mapping>
</web-app>